@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Service>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
	Layout = "master.cshtml";
}

<h1>@Model.Header</h1>
<p>@Model.Excerpt</p>
<p>@Model.Content</p>

@if(Model.Tools != null)
{
    foreach(Tool tool in Model.Tools)
    {
        <a href="@tool.Url">
            <div>
                <h2>@tool.Header</h2>
                <p>@tool.Excerpt</p>
            </div>
        </a>
        
    }
}